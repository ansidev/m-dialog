{
  "name": "vue-m-dialog",
  "description": "A dialog component for vue.",
  "version": "2.0.2",
  "private": false,
  "main": "dist/index.umd.js",
  "types": "types/index.d.ts",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build:docs": "vue-cli-service build",
    "build:lib": "vue-cli-service lint && vue-cli-service build --dest dist --target lib --name index src/index.js",
    "lint": "vue-cli-service lint",
    "open": "dev serve -o docs"
  },
  "files": [
    "dist",
    "src",
    "types"
  ],
  "website": "https://lanyueos.com",
  "github": "https://github.com/mengdu/m-dialog",
  "homepage": "https://mengdu.github.io/m-dialog",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mengdu/m-dialog"
  },
  "bugs": {
    "url": "https://github.com/mengdu/m-dialog/issues"
  },
  "author": {
    "name": "Lanyue",
    "email": "lanyueos@qq.com"
  },
  "devDependencies": {
    "@dove-ui/button": "^0.1.1",
    "@vue/cli-plugin-babel": "^4.5.6",
    "@vue/cli-plugin-eslint": "^4.5.6",
    "@vue/cli-service": "^4.5.6",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.6.5",
    "eslint": "^7.10.0",
    "eslint-plugin-vue": "^7.0.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "vue": "^2.6.12",
    "vue-dotmd-loader": "^0.2.1",
    "vue-router": "^3.4.5",
    "vue-template-compiler": "^2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-unused-vars": "warn"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "eslintIgnore": [
    "**/*.md"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
